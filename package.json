{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "react webpack template",
  "repository": "https://github.com/fortune-cook1e/react-webpack-template.git",
  "author": "fortune_cookie <767077147@qq.com>",
  "license": "MIT",
  "homepage": "https://fortune-cook1e.github.io/react-webpack-template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "dev:analyze": "cross-env NODE_ENV=development USE_ANALYZE=1 webpack serve --config ./config/webpack.dev.js",
    "build:analyze": "cross-env NODE_ENV=production USE_ANALYZE=1 webpack --config config/webpack.prod.js",
    "lint": "cross-env eslint src --ext .js,.ts,.jsx,.tsx --cache",
    "lint-fix": "cross-env eslint src --ext .js,.jsx --fix",
    "stylelint": "stylelint src *.less --cache syntax,less",
    "stylelint-fix":"stylelint src *.less --fix",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --verbose && yarn stylelint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-alloy": "^3.10.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.2.0",
    "husky": "^4.3.8",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss-loader": "^5.0.0",
    "react-refresh": "^0.9.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.2",
    "webpack": "^5.24.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
